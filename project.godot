;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ExerciciosDeProgramacao"
config/features=PackedStringArray("4.5", "Mobile")
config/icon="res://icon.svg"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/do_zero_ao_gd_script/plugin.cfg")

[internationalization]

locale/translations=PackedStringArray("res://translations/pt_BR.po", "res://translations/en.po")
locale/translations_pot_files=PackedStringArray("res://addons/do_zero_ao_gd_script/PainelEditor/PainelEditor.tscn", "res://addons/do_zero_ao_gd_script/PainelEditor/PainelTestRunner/PainelTestRunner.tscn", "res://addons/do_zero_ao_gd_script/PainelEditor/ListaDeExercicios/listas_de_exercicios.tscn", "res://addons/do_zero_ao_gd_script/PainelEditor/Ebook/Ebook.tscn")
